    padding: 9px 15px;
    color: rgb(245, 240, 250);
    font-size: 24px;
    -unity-font-style: bold;
    transition-duration: 0.15s;
    flex-grow: 1;
    -unity-font-definition: url("project://database/Assets/_Data/Fonts/fa-solid-900.ttf?fileID=12800000&guid=512de696859bfa941b17280a6ca5b068&type=3#fa-solid-900");
}

.auto-notes-button:hover {
    background-color: rgb(140, 100, 200);
    border-color: rgb(160, 120, 220);
}

.auto-notes-button:active {
