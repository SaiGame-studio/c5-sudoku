}

.light-mode .sudoku-cell--hint {
    background-color: rgb(200, 255, 220);
    border-color: rgb(50, 180, 80);
    border-width: 2px;
}

.light-mode .sudoku-cell--hint > .sudoku-cell-label {
    color: rgb(20, 120, 50);
}

.light-mode .sudoku-cell--victory {
    background-color: rgb(255, 248, 200);
