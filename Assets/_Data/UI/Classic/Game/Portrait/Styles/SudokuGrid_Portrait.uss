.difficulty-control-panel {
    flex-direction: column;
    align-items: center;
    justify-content: center;
    padding: 15px;
    margin-top: 12px;
}

.difficulty-top-row {
    flex-direction: row;
    align-items: center;
    justify-content: center;
    margin-bottom: 12px;
}

.difficulty-dropdown-stars {
    flex-direction: row;
    align-items: center;
    padding: 6px 9px;
    border-radius: 8px;
    background-color: rgb(45, 45, 50);
    border-width: 2px;
    border-color: rgb(100, 100, 120);
}

.difficulty-dropdown-star {
    font-size: 24px;
    color: rgb(70, 70, 80);
    -unity-text-align: middle-center;
    margin: 0 2px;
    transition-duration: 0.15s;
}

.difficulty-dropdown-star:hover {
    color: rgb(255, 210, 50);
}

.difficulty-dropdown-star--active {
    color: rgb(255, 210, 50);
}

.generate-button {
    background-color: rgb(50, 100, 180);
    border-color: rgb(80, 120, 200);
    border-width: 2px;
    border-radius: 8px;
    padding: 9px 15px;
    color: rgb(240, 240, 250);
    font-size: 24px;
    -unity-font-style: bold;
    transition-duration: 0.15s;
    min-width: 48px;
    min-height: 48px;
    margin-left: 9px;
    -unity-font-definition: url("project://database/Assets/_Data/Fonts/fa-solid-900.ttf?fileID=12800000&guid=512de696859bfa941b17280a6ca5b068&type=3#fa-solid-900");
}

.generate-button:hover {
